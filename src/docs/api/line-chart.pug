
extends components

block variables
  - var pageID = 'api/line-chart'
  - var pageTitle = 'Line Chart'
  - var prism = true

block article
  h3.ui-header-primary= pageTitle
  .ui-container-grid
    .ui-grid-50.ui-mobile-expand
      #line-chart-default
    .ui-grid-50.ui-mobile-expand
      #line-chart-customized
  div
    pre: code.language-markup.
      &lt;div id="line-chart-default"&gt;&lt;/div&gt;
      &lt;div id="line-chart-customized"&gt;&lt;/div&gt;

      &lt;script&gt;
        var data = [];
        d3.lineChart(data, { id: 'line-chart-default' });
        d3.lineChart(data, {
          id: 'line-chart-customized',
          renderer: 'canvas'
        });
      &lt;/script&gt;

block append foot
  script.
    var data = [];
    d3.lineChart(data, { id: 'line-chart-default' });
    d3.lineChart(data, {
      id: 'line-chart-customized',
      renderer: 'canvas'
    });
